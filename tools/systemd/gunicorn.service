[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/terra
ExecStart=/home/ubuntu/.local/share/virtualenvs/terra-XVGhZdP0/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/terra/terra.sock terra.wsgi:application

[Install]
WantedBy=multi-user.target
